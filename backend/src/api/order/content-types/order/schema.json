{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish.dish",
      "mappedBy": "order"
    },
    "total": {
      "type": "decimal"
    },
    "address": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "order"
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "paid",
        "pending",
        "failed"
      ]
    }
  }
}
